{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The Laravel Framework.",
    "keywords": [
        "framework",
        "laravel"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "barryvdh/laravel-dompdf": "^2.0.0",
        "deployer/deployer": "^7.1",
        "guzzlehttp/guzzle": "^7.3",
        "internetcode/laravel-user-settings": "^1.0",
        "intervention/image": "^2.7",
        "j2nlab/laravel-simple-version": "dev-update-repo",
        "jenssegers/agent": "^2.6",
        "laracasts/utilities": "^3.2",
        "laravel/fortify": "^1.16",
        "laravel/framework": "^10.0",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "mcamara/laravel-localization": "^1.8",
        "mollie/laravel-mollie": "^2.22",
        "pragmarx/google2fa-laravel": "^2.0",
        "realrashid/sweet-alert": "^6.0",
        "simplesoftwareio/simple-qrcode": "^4.2",
        "spatie/laravel-backup": "^8.1",
        "spatie/laravel-google-calendar": "^3.5",
        "spatie/laravel-permission": "^5.8",
        "willvincent/feeds": "^2.5"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "itsgoingd/clockwork": "^5.1",
        "jasonmccreary/laravel-test-assertions": "^2.0",
        "laravel-shift/factory-generator": "dev-master",
        "laravel/pint": "^1.2",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^6.1",
        "phpunit/phpunit": "^9.5.10",
        "spatie/laravel-ignition": "^2.0"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ]
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/platformdroneracingnl/laravel-simple-version"
        }
    ]
}
